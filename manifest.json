{
  "name": "Chromac",
  "version": "0.0.1",
  "description": "Let's do this Safari style.",
  "icon": "images/chromac.png",
  "background_page": "chromac.html",
  "content_scripts": [{
     "all_frames": true,
     "js": [ "chromac.js" ],
     "matches": [ "http://*/*", "https://*/*" ],
     "run_at": "document_idle"
  }],
 "permissions": [ "bookmarks", "tabs", "http://*/", "https://*/" ]

}